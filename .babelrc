{
    "presets": [
        [
            "es2015",
            {
                "modules": false
            }
        ],
        "stage-0",
        "stage-2",
        "react"
    ],
    "plugins": [
        "transform-decorators-legacy",
        "syntax-dynamic-import",
        "transform-async-to-generator",
        "transform-regenerator",
        "transform-runtime",
        "transform-class-properties"
    ],
    "env": {
        "production": {
            "plugins": ["transform-react-remove-prop-types"]
        }
    }
}
